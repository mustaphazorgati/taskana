<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<definitions id="definitions" name="definitions" namespace="http://camunda.org/schema/1.0/dmn" xmlns="http://www.omg.org/spec/DMN/20151101/dmn.xsd">
  <decision id="workbasketRouting" name="Workbasket Routing">
    <decisionTable id="workbasketRouting_decisionTable">
      <input id="workbasketRouting_decisionTable-input_classificationName" label="Classification name">
        <inputExpression id="workbasketRouting_decisionTable-inputExpression_classificationName" typeRef="string">
          <text>task.classificationSummary.name</text>
        </inputExpression>
      </input>
      <output id="workbasketRouting_decisionTable-output_workbasketKey" label="Workbasket key" name="workbasketKey" typeRef="string"/>
      <output id="workbasketRouting_decisionTable-output_domain" label="Domain" name="domain" typeRef="string"/>
      <rule id="workbasketRouting_decisionTable-rule_1">
        <inputEntry id="workbasketRouting_decisionTable-rule_1-inputEntry_classificationName">
          <text>"Widerruf"</text>
        </inputEntry>
        <outputEntry id="workbasketRouting_decisionTable-rule_1-outputEntry_workbasketKey">
          <text>"GPK_KSC"</text>
        </outputEntry>
        <outputEntry id="workbasketRouting_decisionTable-rule_1-outputEntry_domain">
          <text>"DOMAIN_A"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
